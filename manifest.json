{

  "manifest_version": 2,
  "name": "Quizlet Bypass",
  "version": "1.1.5",
  "description": "Attempts to bypass the paywalls on Quizlet.",
  "content_scripts": [
    {
      "matches": ["https://quizlet.com/*"],
      "js": ["quizlet.js"]
    }
  ],
  "web_accessible_resources": [
    "inject.js"
  ],
  "browser_specific_settings": {
    "gecko": {
      "id": "quizlet-bypass@jtpotato.github.io"
    }
  }
}